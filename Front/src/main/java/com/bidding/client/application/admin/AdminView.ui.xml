<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:m="urn:import:com.bidding.client.widget">

    <b:Container paddingTop="30">
        <b:Row>
            <b:Column size="XS_12">
                <b:Panel>
                    <b:PanelHeader>
                        <b:Heading size="H3" text="Users"/>
                    </b:PanelHeader>
                    <b:PanelBody>
                        <m:MyCellTable bordered="true" condensed="true" striped="true" hover="true"
                                       ui:field="userTable"/>
                        <b:Pagination ui:field="userTablePagination"/>
                    </b:PanelBody>
                </b:Panel>
            </b:Column>
        </b:Row>
    </b:Container>
</ui:UiBinder>
